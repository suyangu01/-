
.register .content{
  width: 100%;
}

.register .content .register_logo{
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 114rpx 0 52rpx;
}
.register .content .register_logo image{
  width: 320rpx;
  height: 118rpx;
}


/*信息  */
.register .content .info{
  margin: 0 45rpx;
}

.register .content .info .input-wrap {
  border-bottom: 2rpx solid #f2f2f2;
  display: flex;
}
.register .content .info .input-wrap input{
  padding: 54rpx 10rpx 38rpx;
}

.info .input-wrap .placeholder_style{
  font-size: 30rpx;
  color: #8f8f8f;
}

.info .input-wrap .btn-code {
	font-size: 30rpx;
	letter-spacing: -1px;
	color: #f4a110;
  height: 120rpx;
  padding-top: 54rpx;
}


/*协议  */
.protocol{
  margin: 22rpx 0 0 48rpx;
  display: flex;
}
.protocol .protocol-icon{  
  width: 50rpx;
  height: 50rpx;
  margin-top: 6rpx; 
  text-align: center;
}
.protocol .protocol-icon image{
  width: 26rpx;
  height: 26rpx;
}

.protocol .protocol-texts{
  display: flex;
  font-size: 22rpx;
  margin: 10rpx 0 0 12rpx;
}
.protocol .protocol-texts .text{
  font-size: 22rpx;
  color: #41cff2;
}

/*注册按钮  */
.register-btn{
  font-size: 30rpx;
  background-color: #f5141b;
  color: #fff;
  width: 400rpx;
  height: 70rpx;
  line-height: 70rpx;
  text-align: center;
  margin: 89rpx auto 245rpx auto;
}